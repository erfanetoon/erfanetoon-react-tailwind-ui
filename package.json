{
    "name": "@erfanetoon/react-tailwind-ui",
    "version": "2.0.1",
    "description": "Tailwindcss Components for React",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "build:cjs": "swc ./src -d ./dist --config-file .swcrc",
        "move:cjs": "mv ./dist/src/* ./dist/",
        "build:dts": "tsc --declaration --declarationMap --emitDeclarationOnly",
        "build": "rm -rf ./dist && npm run build:cjs && npm run move:cjs && npm run build:dts",
        "pre:publish": "cp ./package.json ./dist && cp ./README.md ./dist",
        "lint:check": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.json --no-eslintrc",
        "prettier:check": "prettier -c ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/erfanetoon/react-tailwind-ui.git"
    },
    "author": "erfanetoon",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/erfanetoon/react-tailwindnpm -ui/issues"
    },
    "homepage": "https://github.com/erfanetoon/react-tailwind-ui#readme",
    "peerDependencies": {
        "@floating-ui/react": "~0.26.0",
        "@types/node": ">=18.0.0",
        "@types/react": "^16 || ^17 || ^18",
        "@types/react-dom": "^16 || ^17 || ^18",
        "classnames": "^2",
        "deepmerge": "^4",
        "framer-motion": "^11",
        "material-ripple-effects": "^2",
        "react": "^16 || ^17 || ^18",
        "react-dom": "^16 || ^17 || ^18",
        "react-icons": "^5",
        "react-merge-refs": "^2",
        "react-responsive": "^10",
        "tailwind-merge": "^2",
        "tailwindcss": "^3"
    },
    "devDependencies": {
        "@floating-ui/react": "^0.26.28",
        "@swc/cli": "^0.5.0",
        "@swc/core": "^1.9.2",
        "@types/node": "^18.19.64",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@typescript-eslint/eslint-plugin": "^8.14.0",
        "@typescript-eslint/parser": "^8.14.0",
        "classnames": "^2.5.1",
        "deepmerge": "^4.3.1",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "framer-motion": "^11.11.17",
        "material-ripple-effects": "^2.0.1",

        "prettier": "^3.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.3.0",
        "react-merge-refs": "^2.1.1",
        "react-responsive": "^10.0.0",
        "tailwind-merge": "^2.5.4",
        "tailwindcss": "^3.4.16",
        "typescript": "^5.6.3"

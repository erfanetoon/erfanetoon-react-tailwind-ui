{
    "name": "@erfanetoon/react-tailwind-ui",
    "version": "1.5.2",
    "description": "Tailwindcss Components for React",
    "main": "index.ts",
    "types": "index.d.ts",
    "scripts": {
        "build:cjs": "swc ./src -d ./dist --config-file .swcrc",
        "build:dts": "tsc --declaration --declarationMap --emitDeclarationOnly",
        "move:cjs": "mv ./dist/src/* ./dist/",
        "copy": "cp ./package.json ./dist && cp ./README.md ./dist",
        "build": "rm -rf ./dist && npm run build:cjs && npm run move:cjs && npm run build:dts",
        "lint:check": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.json --no-eslintrc",
        "prettier:check": "prettier -c .",
        "dev": "npm run build && rm -rf ../faradid-web/node_modules/@erfanetoon/react-tailwind-ui && cp -rf dist ../faradid-web/node_modules/@erfanetoon/react-tailwind-ui"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/erfanetoon/react-tailwind-ui.git"
    },
    "author": "erfanetoon",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/erfanetoon/react-tailwindnpm -ui/issues"
    },
    "homepage": "https://github.com/erfanetoon/react-tailwind-ui#readme",
    "dependencies": {
        "@floating-ui/react": "^0.26.28",
        "classnames": "^2.5.1",
        "deepmerge": "^4.3.1",
        "framer-motion": "^11.11.17",
        "material-ripple-effects": "^2.0.1",
        "react": "^16 || ^17 || ^18",
        "react-dom": "^16 || ^17 || ^18",
        "react-icons": "^5.3.0",
        "react-merge-refs": "^2.1.1",
        "tailwind-merge": "^2.5.4"
    },
    "peerDependencies": {
        "@types/react": "^16 || ^17 || ^18",
        "@types/react-dom": "^16 || ^17 || ^18",
        "react": "^16 || ^17 || ^18",
        "react-dom": "^16 || ^17 || ^18"
    },
    "devDependencies": {
        "@swc/cli": "^0.5.0",
        "@swc/core": "^1.9.2",
        "@types/node": "^18.19.64",
        "@types/react": "^16 || ^17 || ^18",
        "@types/react-dom": "^16 || ^17 || ^18",
        "@typescript-eslint/eslint-plugin": "^8.14.0",
        "@typescript-eslint/parser": "^8.14.0",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "path": "^0.12.7",
        "prettier": "^3.3.3",
        "typescript": "^5.6.3"
    },
    "keywords": [
        "tailwind",
        "tailwind react",
        "javascript",
        "react",
        "next",
        "create react app",
        "tailwind",
        "tailwind components",
        "tailwind elements",
        "tailwind library",
        "react library",
        "react components library",
        "tailwind sections",
        "tailwind css",
        "tailwind ui",
        "tailwind css react",
        "tailwind css next"
    ]
}
